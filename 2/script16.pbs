#!/bin/bash

#PBS -l procs=16

cd $PBS_O_WORKDIR

echo "0, 10^3, 10^3, 10, 16 processes"
mpirun -n 16 ./stat 0 1000 1000 10 16

echo "0, 10^3, 10^3, 1000, 16 processes"
mpirun -n 16 ./stat 0 1000 1000 1000 16

echo "0, 10^3, 10^9, 10, 16 processes"
mpirun -n 16 ./stat 0 1000 1000000000 10 16

echo "0, 10^3, 10^9, 1000, 16 processes"
mpirun -n 16 ./stat 0 1000 1000000000 1000 16

exec 1>$PBS_O_WORKDIR/out  2>$PBS_O_WORKDIR/err